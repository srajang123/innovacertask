<div class="contact1">
        <div class="container-contact1">
            <div class="contact1-pic js-tilt" data-tilt>
                <img src="images/img-01.png" alt="IMG">
            </div>
            {{#if getData}}
            <form class="contact1-form validate-form" name="checkout-form" method="post" action="/fetch">
                <span class="contact1-form-title">
					Welcome...
				</span>

                <div class="wrap-input1 validate-input" data-validate="Valid email is required: ex@abc.xyz">
                    <input class="input1" type="text" name="email" placeholder="Email">
                    <span class="shadow-input1"></span>
                </div>

                <div class="container-contact1-form-btn">
                    <button class="contact1-form-btn">
						<span>
							View
							<i class="fa fa-long-arrow-right" aria-hidden="true"></i>
						</span>
					</button>
                </div>
            </form>
            {{else}}
            <form class="contact1-form validate-form views" name="checkout-form" method="post" action="/checkout">
                <input type="hidden" name="user" value="{{data.gemail}}">
                <span class="contact1-form-title">
					Hope You Had Fun...
				</span>
                <div class="data-view">
                    <div class="view">
                        <span class="title">Name: </span>
                        <span class="value">{{data.gname}}</span>
                    </div>
                    <div class="view">
                        <span class="title">Email: </span>
                        <span class="value">{{data.gemail}}</span>
                    </div>
                    <div class="view">
                        <span class="title">Mobile Number: </span>
                        <span class="value">{{data.gmobile}}</span>
                    </div>
                    <div class="view">
                        <span class="title">Check-In: </span>
                        <span class="value">{{data.checkin}}</span>
                    </div>
                </div>
                <br/>
                <br/>
                <div class="container-contact1-form-btn">
                    <button class="contact1-form-btn">
						<span>
							CheckOut
							<i class="fa fa-long-arrow-right" aria-hidden="true"></i>
						</span>
					</button>
                </div>
            </form>
            {{/if}}
        </div>
    </div>